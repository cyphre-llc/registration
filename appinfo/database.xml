<?xml version="1.0" encoding="ISO-8859-1" ?>
<database>
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<charset>latin1</charset>
	<table>
		<name>*dbprefix*pending_regist</name>
		<declaration>
			<field>
				<name>email</name>
				<type>text</type>
				<default></default>
				<notnull>true</notnull>
				<length>64</length>
			</field>
			<field>
				<name>token</name>
				<type>text</type>
				<default></default>
				<notnull>false</notnull>
				<length>256</length>
			</field>
			<field>
				<name>requested</name>
				<type>integer</type>
				<default></default>
				<notnull>true</notnull>
			</field>
			<index>
				<name>pending_regist_pKey</name>
				<primary>true</primary>
				<field>
					<name>requested</name>
					<sorting>descending</sorting>
				</field>
			</index>
		</declaration>
	</table>
</database>
